{
    "name": "1tomany/pdf-to-image-bundle",
    "description": "Symfony bundle for the 1tomany/pdf-to-image library",
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "keywords": [
        "pdf",
        "poppler",
        "pdftoppm",
        "xpdf"
    ],
    "require": {
        "php": ">=8.4",
        "1tomany/pdf-to-image": "^0.0.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\PdfToImageBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
